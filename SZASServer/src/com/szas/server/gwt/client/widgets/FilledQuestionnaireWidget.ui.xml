<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.scroll {
			border-top: 2px solid #6F7277;
			border-bottom: 2px solid #6F7277;
		}
		
		.header {
			font-size: 26px;
		}
	</ui:style>
	<g:RootLayoutPanel width="" height="">
		<g:layer left="0px" width="88px" top="0px" height="25px">
			<g:Button ui:field="saveButton">Save</g:Button>
		</g:layer>
		<g:layer left="88px" width="88px" top="0px" height="25px">
			<g:Button ui:field="deleteButton">Delete</g:Button>
		</g:layer>
		<g:layer left="0px" top="25px" height="30px" right="0px">
			<g:Label styleName="{style.header}" text="Name Questionnaire"
				ui:field="questionnaireName" />
		</g:layer>
		<g:layer left="0px" top="70px" bottom="0px" right="0px">
			<g:ScrollPanel styleName="{style.scroll}">
				<g:VerticalPanel width="100%" height="100%"
					ui:field="verticalPanel" />
			</g:ScrollPanel>
		</g:layer>
	</g:RootLayoutPanel>
</ui:UiBinder> 